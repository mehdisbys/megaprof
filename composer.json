{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "7.1.2",
    "laravel/framework": "5.6.*",
    "classpreloader/classpreloader": "~3.0",
    "laravelcollective/html": "5.6",
    "nesbot/carbon": "~1.14",
    "spatie/laravel-tail": "dev-master",
    "intervention/image": "^2.3",
    "predis/predis": "^1.1",
    "laravel/socialite": "^3.0",
    "cviebrock/eloquent-sluggable": "^4.2",
    "guzzlehttp/guzzle": "^6.2",
    "laravel/dusk": "^3.0",
    "orangehill/iseed": "2.2",
    "doctrine/dbal": "^2.5",
    "spatie/laravel-sitemap": "5.2.0",
    "spatie/laravel-activitylog": "2.5.1",
    "gbrock/laravel-table": "dev-master"

  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~6.0",
    "symfony/dom-crawler": "~4.0",
    "symfony/css-selector": "~4.0",
    "xethron/migrations-generator": "^2.0",
    "filp/whoops": "~2.0"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/jamisonvalenta/Laravel-4-Generators.git"
    }
  ],
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Reminders\\": "app/reminders",
      "Taelam\\": "taelam"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled"
    ],
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
